{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2024"
    ],
    "target": "ES2024",
    "incremental": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      "@payload-config": ["../../packages/payload/payload.config.ts"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "next-env.d.ts"
  ],
  "files": [],
  "references": [
    {
      "path": "../../packages/payload"
    }
  ],
  "exclude": [
    "node_modules"
  ]
}
