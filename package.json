{
	"name": "apologetik-projekt",
	"version": "3.0.0",
	"author": "Silas",
	"license": "ISC",
	"private": true,
	"type": "module",
	"sideEffects": false,
	"packageManager": "pnpm@10.14.0",
	"dependencies": {
		"@payloadcms/db-postgres": "3.49.0",
		"payload": "3.49.0"
	},
	"scripts": {
		"preinstall": "npx only-allow pnpm"
	},
	"engines": {
		"node": ">=22.18.0",
		"pnpm": ">=10.14.0"
	},
	"pnpm": {
		"peerDependencyRules": {
			"allowAny": [
				"react",
				"react-dom"
			]
		},
		"onlyBuiltDependencies": [
			"@sentry/cli",
			"@tailwindcss/oxide",
			"esbuild",
			"iframe-resizer",
			"sharp",
			"unrs-resolver"
		]
	}
}
